// Cumulative sum of an array.

class CumulativeSum
{
    public static void main(String [] args)
    {
        int cumsum = 0;
        
        int arr[]={5,6,22,4,18,15,12,17,10,11,19};
        
        for(int i = 0; i < arr.length; i++)
        {
            cumsum = cumsum+arr[i];
            System.out.println(cumsum+" ");
        }
    }
}
